{% extends 'base.html.twig' %}

{% block body %}
    <h1>TravelAgency</h1>

    <table>
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ travelAgency.id }}</td>
        </tr>
        <tr>
            <th>Idelemento</th>
            <td>{{ travelAgency.idElemento }}</td>
        </tr>
        <tr>
            <th>Idcontenuto</th>
            <td>{{ travelAgency.idContenuto }}</td>
